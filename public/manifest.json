{
  "manifest_version": 2,

  "name": "Note Tab",
  "short_name": "Note Tab",
  "version": "2.1",
  "description": "Chrome Note Tab: simple note storage.",

  "permissions": [
      "contextMenus",
      "tabs"
  ],
  "chrome_url_overrides": {
      "newtab": "index.html"
  },
  "offline_enabled": true
}